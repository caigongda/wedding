.detail-list-wrap{
  display: flex;
  padding: 0 16rpx;
  flex-wrap: wrap;
  justify-content: space-between;
  display: -webkit-flex;
  margin-top:88rpx;
}
.detail-h115{
  width: 100%;
  height: 115rpx;
}
.list-img{
  width: 347rpx;
  height: 475rpx;
  margin-top: 29rpx;
}
.list-img:nth-child(2n){
  margin-right: 0;
}
.list-img image{
  width: 100%;
  height: 100%;
}
.detail-btn-wrap{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 115rpx;
  display: flex;
  padding: 29rpx 0;
  box-sizing: border-box;
  justify-content: space-around;
  background: #fff;
}
.detail-btn{
  width: 259rpx;
  height: 62rpx;
  text-align: center;
  line-height: 62rpx;
  color: #fff;
  font-size: 28rpx;
  border-radius: 32rpx;
  background: #F46954;
}
.circle-float{
  color: #ffffff;
  position: fixed;
  bottom: 200rpx;
  right: 50rpx;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 50%;
  font-size: 58rpx;
  background: #F46954;
}
.detail-wrap-header{
  width: 100%;
  position: fixed;
  top: 0;
  height: 88rpx;
  line-height: 88rpx;
  background: #fff;
  color: #F46954;
  padding:0 40rpx;
  box-sizing: border-box;
  font-size: 58rpx;
  text-align: right;
  border-bottom: 1rpx solid #d1d1d1;
}