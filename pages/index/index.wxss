/**index.wxss**/
.index-slide-image{
  width: 100%;
  height: 100%;
}
.index-swiper-top{
  height: 380rpx;
}
.index-card-wrap{
  width: 350rpx;
  height: 208rpx;
  border-radius: 13rpx;
  background: #fff;
  margin:25rpx 12rpx 0 13rpx;
  float: left;
  box-shadow: 0 2rpx 11rpx 5rpx #D9D9D9;
}
.index-card-img{
  width: 100%;
  height: 163rpx;
}
.index-card-img image{
  border-radius: 13rpx 13rpx 0 0;
  width:100%;
  height:100%;
}
.index-card-title{
  font-size: 24rpx;
  color: #888888;
  text-align: center;
  height: 45rpx;
  line-height: 45rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}