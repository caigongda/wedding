.picture-list-wrap{
  display: flex;
  padding: 16rpx;
  width: 100%;
  height: 130rpx;
  box-sizing: border-box;
  align-items: center;
  border-bottom: 1px solid #E9E9E9;
  justify-content: space-between;
}
.list-wrap-left{
  display: flex;
  align-items: center;
}
.wrap-left-img{
  width: 138rpx;
  height: 100rpx;
}
.wrap-left-img image{
  width: 100%;
  height: 100%;
} 
.wrap-right-text{
  margin-left: 30rpx;
  color: #4B4B4B;
  font-size: 35rpx;
}