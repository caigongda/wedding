@keyframes fresh{
  100%{
    transform: rotate(360deg);
  }
}
.fresh-content{
  animation: fresh 500ms infinite linear;
}
.over-flow-text{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.over-two-row{
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
::-webkit-scrollbar { width: 0; height: 0; color: transparent; }
button {
  background-color: #fff;
}
button::after {
  border: none;
}
.public-btn-wrap{
  display: flex;
  justify-content: space-around;
}
.public-btn{
  width: 259rpx;
  height: 62rpx;
  text-align: center;
  line-height: 62rpx;
  color: #fff;
  font-size: 28rpx;
  border-radius: 32rpx;
  background: #F46954;
}
page{
  width: 100%;
  height: 100%;
}